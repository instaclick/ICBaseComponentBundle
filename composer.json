{
    "name": "instaclick/base-component-bundle",
    "description": "This bundle provides basic component support for Dating Engine",
    "keywords": ["base", "component", "widget"],
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "InstaClick Inc.",
            "homepage": "http://www.instaclick.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://satis.dev:8080/"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-0": { "IC\\Bundle\\Base\\ComponentBundle": "" }
    },
    "target-dir": "IC/Bundle/Base/ComponentBundle",
    "minimum-stability": "dev"
}
